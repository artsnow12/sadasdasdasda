    <script>
        const stringType = "Hello, World!";
        const numberType = 42;
        const bigintType = 12345678901234567890n;
        const booleanType = true;
        const undefinedType = undefined;

        console.log("Тип stringType:", typeof stringType);
        console.log("Тип numberType:", typeof numberType);
        console.log("Тип bigintType:", typeof bigintType);
        console.log("Тип booleanType:", typeof booleanType);
        console.log("Тип undefinedType:", typeof undefinedType);
    </script>
</body>
